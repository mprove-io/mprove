{
  "name": "mprove-backend",
  "author": "Alexander Kalitenya <akalitenya@mprove.io>",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mprove-io/mprove.git"
  },
  "scripts": {
    "clean": "rimraf -- dist ormlogs.log .awcache",
    "lint": "tslint --config tslint.json -e 'node_modules/**' \"{src,test}/**/*.ts\"",
    "circular": "madge --circular --extensions ts ./",
    "files:check": "npm-run-all --parallel lint circular",
    "build": "npm run files:check && npm run clean && tsc",
    "start": "npm run build && npm run watch",
    "server": "node dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "watch-ts": "tsc -w",
    "jest": "JEST=yes jest",
    "test": "JEST=yes jest --forceExit"
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {},
    "testMatch": [
      "**/test/**/*.test.js"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@google-cloud/bigquery": "2.0.6",
    "@types/express-jwt": "^0.0.40",
    "@types/nodemailer": "^4.6.5",
    "@types/nodemailer-mailgun-transport": "^1.3.3",
    "@types/passport": "^1.0.0",
    "@types/passport-local": "^1.0.33",
    "@types/ws": "^6.0.1",
    "async": "^2.6.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "clone-regexp": "^2.1.0",
    "compression": "^1.7.1",
    "cron": "1.7.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.1",
    "express-validator": "^4.3.0",
    "express-ws": "^4.0.0",
    "fs-extra": "^7.0.0",
    "jsonwebtoken": "^8.5.0",
    "jwks-rsa": "^1.3.0",
    "lusca": "^1.5.2",
    "morgan": "^1.7.0",
    "mysql": "^2.16.0",
    "nanoid": "^1.3.0",
    "nodegit": "0.24.0-alpha.1",
    "nodemailer": "^5.1.1",
    "nodemailer-mailgun-transport": "^1.4.0",
    "p-iteration": "^1.1.7",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg-promise": "^8.7.2",
    "request": "^2.83.0",
    "rimraf": "^2.6.2",
    "typeorm": "^0.2.7",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/async": "^2.0.45",
    "@types/compression": "^0.0.35",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.11.1",
    "@types/express-ws": "^3.0.0",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^22.1.3",
    "@types/lusca": "^1.5.0",
    "@types/morgan": "^1.7.35",
    "@types/node": "^9.4.6",
    "@types/nodegit": "^0.22.3",
    "@types/uuid": "^3.4.4",
    "concurrently": "^3.5.1",
    "jest": "23.6.0",
    "madge": "4.0.1",
    "nodemon": "^1.13.0",
    "npm-run-all": "^4.1.3",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "1.16.0",
    "typescript": "^3.2.0"
  }
}
