{
  "name": "@mprove/mcli",
  "version": "11.0.96-dev",
  "author": "Alexander Kalitenya <akalitenya@mprove.io>",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "imports": {
    "#mcli/*": "./src/*",
    "#common/*": "@mprove/common/*",
    "#node-common/*": "@mprove/node-common/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mprove-io/mprove.git"
  },
  "bin": "main.js",
  "scripts": {
    "lint": "biome lint src",
    "circular": "madge --circular .",
    "build": "bun build src/main.ts --compile --outfile dist/bin/mprove",
    "build:linux": "bun build src/main.ts --compile --target=bun-linux-x64 --outfile dist/bin/mprove-linux-x64",
    "e2e": "dotenv -e ../.env -- bun test --concurrent 4 --timeout 60000 src",
    "clean-node": "rimraf --glob \"node_modules/*\" \"node_modules/.[!.]*\"",
    "clean-dist": "rimraf --glob \"dist/*\" \"dist/.[!.]*\"",
    "clean-bun": "rimraf --glob \"bun.lock\" \".bun/*\" \".bun/.[!.]*\""
  },
  "dependencies": {
    "@mprove/common": "file:../libs/common",
    "@mprove/node-common": "file:../libs/node-common",
    "@nestjs/common": "11.1.0",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.67.2",
    "@opentelemetry/exporter-trace-otlp-http": "0.208.0",
    "@opentelemetry/resources": "2.2.0",
    "@opentelemetry/sdk-node": "0.208.0",
    "@opentelemetry/semantic-conventions": "1.38.0",
    "async-retry": "1.3.3",
    "axios": "1.13.1",
    "class-transformer": "0.3.1",
    "class-transformer-validator": "0.9.1",
    "class-validator": "0.14.2",
    "clipanion": "3.2.1",
    "date-fns": "4.1.0",
    "date-fns-tz": "3.2.0",
    "dotenv": "17.2.3",
    "fast-deep-equal": "3.1.3",
    "fs-extra": "11.2.0",
    "js-yaml": "4.1.1",
    "nanoid": "3.3.10",
    "nest-winston": "1.9.4",
    "simple-git": "3.30.0",
    "p-iteration": "1.1.8",
    "prettyjson": "1.2.5",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "tslib": "2.7.0",
    "typanion": "3.14.0",
    "winston": "3.14.2"
  },
  "devDependencies": {
    "@types/async-retry": "1.4.9",
    "@types/bun": "latest",
    "@types/fs-extra": "9.0.13",
    "@types/js-yaml": "4.0.9",
    "ava": "6.4.1",
    "dotenv-cli": "11.0.0",
    "shx": "0.4.0"
  }
}
