{
  "name": "@mprove/blockml",
  "version": "11.0.96-dev",
  "private": true,
  "scripts": {
    "circular": "madge --circular .",
    "build:prod": "swc src -d dist --source-maps && node build.mjs",
    "serve": "dotenv -e ../../.env -- node --enable-source-maps dist/main.js",
    "start": "dotenv -e ../../.env -- node --watch -r ts-node/register -r tsconfig-paths/register src/main.ts",
    "debug": "dotenv -e ../../.env -- node --inspect=0.0.0.0:9231 --watch -r ts-node/register -r tsconfig-paths/register src/main.ts",
    "build-tests": "rimraf dist/tests && tsc --project tsconfig.test.json",
    "test": "dotenv -e ../../.env -v BLOCKML_LOG_IO=TRUE -v IS_TELEMETRY_ENABLED=FALSE -- ava dist/tests/**/*.spec.js"
  },
  "dependencies": {
    "@mprove/common": "workspace:*",
    "@mprove/node-common": "workspace:*",
    "@nestjs/common": "catalog:",
    "@nestjs/config": "catalog:",
    "@nestjs/core": "catalog:",
    "@nestjs/platform-express": "catalog:",
    "@opentelemetry/api": "catalog:",
    "@opentelemetry/auto-instrumentations-node": "catalog:",
    "@opentelemetry/exporter-trace-otlp-http": "catalog:",
    "@opentelemetry/resources": "catalog:",
    "@opentelemetry/sdk-node": "catalog:",
    "@opentelemetry/semantic-conventions": "catalog:",
    "class-transformer": "catalog:",
    "class-transformer-validator": "catalog:",
    "class-validator": "catalog:",
    "date-fns": "catalog:",
    "@malloydata/db-bigquery": "catalog:",
    "@malloydata/db-duckdb": "catalog:",
    "@malloydata/db-mysql": "catalog:",
    "@malloydata/db-postgres": "catalog:",
    "@malloydata/db-snowflake": "catalog:",
    "@malloydata/db-trino": "catalog:",
    "@malloydata/malloy": "catalog:",
    "@malloydata/malloy-filter": "catalog:",
    "@malloydata/malloy-interfaces": "catalog:",
    "@malloydata/malloy-query-builder": "catalog:",
    "@malloydata/malloy-tag": "catalog:",
    "nanoid": "catalog:",
    "d3-format": "catalog:",
    "walk": "catalog:",
    "date-fns-tz": "catalog:",
    "fs-extra": "catalog:",
    "groupmq": "catalog:",
    "ioredis": "catalog:",
    "js-yaml": "catalog:",
    "nest-winston": "catalog:",
    "nodegit": "catalog:",
    "p-iteration": "catalog:",
    "reflect-metadata": "catalog:",
    "rxjs": "catalog:",
    "tiny-async-pool": "catalog:",
    "tslib": "catalog:",
    "winston": "catalog:"
  },
  "devDependencies": {
    "dotenv-cli": "catalog:",
    "@nestjs/testing": "catalog:",
    "@swc/cli": "catalog:",
    "@types/walk": "catalog:",
    "@types/d3-format": "catalog:",
    "@types/fs-extra": "catalog:",
    "@types/js-yaml": "catalog:",
    "@types/nodegit": "catalog:",
    "@types/tiny-async-pool": "catalog:",
    "ava": "catalog:",
    "esbuild": "catalog:",
    "tsconfig-paths": "catalog:"
  },
  "_moduleAliases": {
    "~blockml": "dist/tests/apps/blockml/src",
    "~common": "dist/tests/libs/common/src",
    "~node-common": "dist/tests/libs/node-common/src"
  }
}
