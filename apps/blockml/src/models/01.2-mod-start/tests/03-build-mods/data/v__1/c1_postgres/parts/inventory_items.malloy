source: inventory_items_table is c1_postgres.table('ecommerce.inventory_items') extend {
  rename: _product_id is product_id
  rename: _actual_cost is actual_cost
  rename: _distribution_center_id is distribution_center_id
}

source: inventory_items is inventory_items_table extend {
  primary_key: inventory_item_id
  
  dimension: actual_cost is _actual_cost::'numeric' 
  rename: product_id is _product_id
  rename: distribution_center_id is _distribution_center_id

  measure: 
    inventory_items_count is count()
    total_actual_cost is actual_cost.sum()
}