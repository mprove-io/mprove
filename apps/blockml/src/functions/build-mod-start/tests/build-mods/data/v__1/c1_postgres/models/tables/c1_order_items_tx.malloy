source: c1_order_items_table is c1_postgres.table('ecommerce.order_items') extend {
  rename: _sale_price is sale_price
  rename: _order_id is order_id
  rename: _inventory_item_id is inventory_item_id
}

source: c1_order_items_tx is c1_order_items_table extend {
  primary_key: order_item_id
  
  dimension: sale_price is _sale_price::'Numeric'
  
  rename: order_id is _order_id
  rename: inventory_item_id is _inventory_item_id
   
  measure:  
    order_items_count is count()
    total_sale_price is sale_price.sum()
    average_sale_price is sale_price.avg()
    least_expensive_item is sale_price.min()
    most_expensive_item is sale_price.max()
}