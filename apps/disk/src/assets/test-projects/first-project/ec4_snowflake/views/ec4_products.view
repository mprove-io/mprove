view: ec4_products
label: Products
connection: c4_snowflake
table: s_db.ecommerce.products

fields:
- dimension: product_id
  sql: product_id
  label: Id
  
- dimension: retail_price
  sql: retail_price
  result: number

- dimension: cost
  sql: cost
  result: number
  
- dimension: category
  sql: category
  
- dimension: brand
  sql: brand
  
- measure: count
  type: count_distinct
  sql: ${product_id}