view: ec2_users
label: Users
connection: c2_clickhouse
table: c_db.users

fields:
- dimension: user_id
  sql: user_id
  label: Id
  
- dimension: age
  result: number
  sql: age

- dimension: age_group 
  sql: |
    CASE 
    WHEN ${age} < 25 THEN '1) Below 25'
    WHEN ${age} >= 25 AND ${age} < 35 THEN '2) 25 to 34'
    WHEN ${age} >= 35 AND ${age} < 45 THEN '3) 35 to 44'
    WHEN ${age} >= 45 AND ${age} < 55 THEN '4) 45 to 54'
    WHEN ${age} >= 55 AND ${age} < 65 THEN '5) 55 to 64'
    WHEN ${age} >= 65 THEN '6) 65 or Above'
    ELSE 'Undefined'
    END

- dimension: city
  sql: city
  
- dimension: email
  sql: email
  
- dimension: first_name
  sql: first_name
  
- dimension: last_name
  sql: last_name  
  
- dimension: has_last_name
  type: yesno_is_true
  sql: ${last_name} IS NOT NULL

- dimension: gender
  sql: gender

- dimension: state
  sql: state
  hidden: true

- dimension: state_name
