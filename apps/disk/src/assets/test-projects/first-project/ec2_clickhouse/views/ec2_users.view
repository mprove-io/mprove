    end

- dimension: traffic_source
  sql: traffic_source  
  
- measure: count
  type: count_distinct
  sql: ${user_id}   
  
- measure: men_count
  type: count_distinct
  sql: CASE WHEN ${gender} = 'Male' THEN ${user_id} ELSE NULL END  
  
- measure: female_count
  type: count_distinct
  sql: CASE WHEN ${gender} = 'Female' THEN ${user_id} ELSE NULL END