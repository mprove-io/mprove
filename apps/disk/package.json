{
  "name": "@mprove/disk",
  "version": "11.0.97-dev",
  "private": true,
  "type": "module",
  "imports": {
    "#disk/*": "./src/*",
    "#common/*": "@mprove/common/*",
    "#node-common/*": "@mprove/node-common/*"
  },
  "scripts": {
    "check": "pnpm typecheck && pnpm lint && pnpm circular",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint src",
    "circular": "madge --circular .",
    "build": "swc src -d dist --source-maps && node build.mjs",
    "serve": "dotenv -e ../../.env -- node --enable-source-maps dist/main.js",
    "start": "dotenv -e ../../.env -- node --import @swc-node/register/esm-register --watch src/main.ts",
    "debug": "dotenv -e ../../.env -- node --import @swc-node/register/esm-register --inspect=0.0.0.0:9230 --watch src/main.ts",
    "test": "dotenv -e ../../.env -v IS_TELEMETRY_ENABLED=FALSE -- ava",
    "clean-node": "rimraf --glob \"node_modules/*\" \"node_modules/.[!.]*\"",
    "clean-dist": "rimraf --glob \"dist/*\" \"dist/.[!.]*\"",
    "clean-turbo": "rimraf --glob \".turbo/*\" \".turbo/.[!.]*\""
  },
  "dependencies": {
    "@mprove/common": "workspace:*",
    "@mprove/node-common": "workspace:*",
    "@nestjs/common": "catalog:",
    "@nestjs/config": "catalog:",
    "@nestjs/core": "catalog:",
    "@nestjs/platform-express": "catalog:",
    "@opentelemetry/api": "catalog:",
    "@opentelemetry/auto-instrumentations-node": "catalog:",
    "@opentelemetry/exporter-trace-otlp-http": "catalog:",
    "@opentelemetry/resources": "catalog:",
    "@opentelemetry/sdk-node": "catalog:",
    "@opentelemetry/semantic-conventions": "catalog:",
    "class-transformer": "catalog:",
    "class-transformer-validator": "catalog:",
    "class-validator": "catalog:",
    "fs-extra": "catalog:",
    "groupmq": "catalog:",
    "ioredis": "catalog:",
    "js-yaml": "catalog:",
    "nest-winston": "catalog:",
    "simple-git": "catalog:",
    "p-iteration": "catalog:",
    "reflect-metadata": "catalog:",
    "rxjs": "catalog:",
    "tslib": "catalog:",
    "winston": "catalog:"
  },
  "devDependencies": {
    "dotenv-cli": "catalog:",
    "@nestjs/testing": "catalog:",
    "@swc/cli": "catalog:",
    "@swc/core": "catalog:",
    "@swc/helpers": "catalog:",
    "@swc-node/register": "catalog:",
    "@types/fs-extra": "catalog:",
    "@types/js-yaml": "catalog:",
    "ava": "catalog:",
    "esbuild": "catalog:"
  }
}
