{
  "name": "@mprove/mcli",
  "version": "11.0.96-dev",
  "author": "Alexander Kalitenya <akalitenya@mprove.io>",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "imports": {
    "#mcli/*": "./src/*",
    "#common/*": "@mprove/common/*",
    "#node-common/*": "@mprove/node-common/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mprove-io/mprove.git"
  },
  "bin": "main.js",
  "scripts": {
    "lint": "biome lint src",
    "circular": "madge --circular .",
    "build:prod": "swc src -d dist --source-maps && node build.mjs && shx cp package.json dist/package.json && shx cp package.json dist/src/package.json",
    "e2e": "dotenv -e ../../.env -v IS_TELEMETRY_ENABLED=FALSE -- ava --concurrency=4",
    "pack": "pkg dist/src",
    "clean-node": "rimraf --glob \"node_modules/*\" \"node_modules/.[!.]*\"",
    "clean-dist": "rimraf --glob \"dist/*\" \"dist/.[!.]*\" && pnpm clean-bin",
    "clean-bin": "rimraf --glob \"bin/*\" \"bin/.[!.]*\"",
    "clean-turbo": "rimraf --glob \".turbo/*\" \".turbo/.[!.]*\""
  },
  "dependencies": {
    "@mprove/common": "workspace:*",
    "@mprove/node-common": "workspace:*",
    "@nestjs/common": "catalog:",
    "@opentelemetry/api": "catalog:",
    "@opentelemetry/auto-instrumentations-node": "catalog:",
    "@opentelemetry/exporter-trace-otlp-http": "catalog:",
    "@opentelemetry/resources": "catalog:",
    "@opentelemetry/sdk-node": "catalog:",
    "@opentelemetry/semantic-conventions": "catalog:",
    "async-retry": "catalog:",
    "axios": "catalog:",
    "class-transformer": "catalog:",
    "class-transformer-validator": "catalog:",
    "class-validator": "catalog:",
    "clipanion": "catalog:",
    "date-fns": "catalog:",
    "date-fns-tz": "catalog:",
    "dotenv": "catalog:",
    "fast-deep-equal": "catalog:",
    "fs-extra": "catalog:",
    "js-yaml": "catalog:",
    "nanoid": "catalog:",
    "nest-winston": "catalog:",
    "nodegit": "catalog:",
    "p-iteration": "catalog:",
    "prettyjson": "catalog:",
    "reflect-metadata": "catalog:",
    "rxjs": "catalog:",
    "tslib": "catalog:",
    "typanion": "catalog:",
    "winston": "catalog:"
  },
  "devDependencies": {
    "dotenv-cli": "catalog:",
    "@swc/cli": "catalog:",
    "@swc-node/register": "catalog:",
    "@swc/core": "catalog:",
    "@swc/helpers": "catalog:",
    "@types/async-retry": "catalog:",
    "@types/fs-extra": "catalog:",
    "@types/js-yaml": "catalog:",
    "@types/nodegit": "catalog:",
    "@yao-pkg/pkg": "catalog:",
    "ava": "catalog:",
    "esbuild": "catalog:",
    "shx": "catalog:"
  },
  "pkg": {
    "scripts": [
      "*.js",
      "**/*.js"
    ],
    "assets": [
      "node_modules/nodegit/build/Release/**/*",
      "node_modules/axios/dist/node/axios.cjs"
    ],
    "outputPath": "bin",
    "compress": "GZip"
  }
}
