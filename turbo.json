{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "check": {
      "dependsOn": ["^check"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "circular": {
      "dependsOn": ["^circular"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "node-serve": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "debug": {
      "cache": false,
      "persistent": true
    },
    "test": {},
    "e2e": {},
    "e2x": {
      "cache": false
    },
    "custom": {
      "cache": false
    },
    "clean-node": {
      "cache": false
    },
    "clean-dist": {
      "cache": false
    },
    "clean-turbo": {
      "cache": false
    }
  }
}
