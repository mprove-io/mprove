{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "lint": {},
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "circular": {},
    "clean-node": {
      "cache": false
    },
    "clean-dist": {
      "cache": false
    },
    "clean-turbo": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "serve": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    },
    "debug": {
      "cache": false,
      "persistent": true
    },
    "test": {},
    "e2e": {},
    "pack": {
      "dependsOn": ["build"]
    }
  }
}
