{
  "version": 2,
  "projects": {
    "disk": {
      "root": "apps/disk",
      "sourceRoot": "apps/disk/src",
      "projectType": "application",
      "prefix": "disk",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/disk",
            "main": "apps/disk/src/main.ts",
            "tsConfig": "apps/disk/tsconfig.app.json",
            "assets": ["apps/disk/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/disk/src/environments/environment.ts",
                  "with": "apps/disk/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "disk:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/disk/**/*.ts"] }
        }
      }
    },
    "nest-app": {
      "root": "apps/nest-app",
      "sourceRoot": "apps/nest-app/src",
      "projectType": "application",
      "prefix": "nest-app",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/nest-app",
            "main": "apps/nest-app/src/main.ts",
            "tsConfig": "apps/nest-app/tsconfig.app.json",
            "assets": ["apps/nest-app/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/nest-app/src/environments/environment.ts",
                  "with": "apps/nest-app/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "nest-app:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/nest-app/**/*.ts"] }
        }
      }
    },
    "nest-app-2": {
      "root": "apps/nest-app-2",
      "sourceRoot": "apps/nest-app-2/src",
      "projectType": "application",
      "prefix": "nest-app-2",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/nest-app-2",
            "main": "apps/nest-app-2/src/main.ts",
            "tsConfig": "apps/nest-app-2/tsconfig.app.json",
            "assets": ["apps/nest-app-2/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/nest-app-2/src/environments/environment.ts",
                  "with": "apps/nest-app-2/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "nest-app-2:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/nest-app-2/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/nest-app-2"],
          "options": {
            "jestConfig": "apps/nest-app-2/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api": {
      "root": "libs/api",
      "sourceRoot": "libs/api/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/api/**/*.ts"] }
        }
      }
    }
  },
  "cli": { "defaultCollection": "@nrwl/nest" },
  "defaultProject": "disk"
}
