{
  "version": 2,
  "projects": {
    "api": {
      "root": "libs/api",
      "sourceRoot": "libs/api/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/api/**/*.ts"]
          }
        },
        "build-tests": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "rimraf \"./dist/tests/api\" && tsc --project libs/api/tsconfig.test.json",
            "outputPath": "dist/tests/api"
          }
        },
        "format": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "prettier \"libs/api/**/*.{ts,js,json,scss,html}\" --write --config .prettierrc.yaml"
          }
        },
        "circular": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "madge --circular libs/api"
          }
        }
      }
    },
    "disk": {
      "root": "apps/disk",
      "sourceRoot": "apps/disk/src",
      "projectType": "application",
      "prefix": "disk",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/disk",
            "main": "apps/disk/src/main.ts",
            "tsConfig": "apps/disk/tsconfig.app.json",
            "assets": ["apps/disk/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "disk:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/disk/**/*.ts"]
          }
        },
        "build-tests": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "rimraf \"./dist/tests/disk\" && tsc --project apps/disk/tsconfig.test.json",
            "outputPath": "dist/tests/disk"
          }
        },
        "test": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx build-tests disk --with-deps",
              "ava --config=ava-js.config.js dist/tests/disk/apps/disk/**/*.spec.js"
            ],
            "parallel": false
          }
        },
        "format": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "prettier \"apps/disk/**/*.{ts,js,json,scss,html}\" --write --config .prettierrc.yaml"
          }
        },
        "circular": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "madge --circular apps/disk"
          }
        }
      }
    },
    "blockml": {
      "root": "apps/blockml",
      "sourceRoot": "apps/blockml/src",
      "projectType": "application",
      "prefix": "blockml",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/blockml",
            "main": "apps/blockml/src/main.ts",
            "tsConfig": "apps/blockml/tsconfig.app.json",
            "assets": ["apps/blockml/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "blockml:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/blockml/**/*.ts"]
          }
        },
        "clean-logs": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "rimraf \"./apps/blockml/src/models/**/logs/*.log\""
          }
        },
        "build-tests": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "rimraf \"./dist/tests/blockml\" && tsc --project apps/blockml/tsconfig.test.json",
            "outputPath": "dist/tests/blockml"
          }
        },
        "test": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx clean-logs blockml",
              "nx build-tests blockml --with-deps",
              "ava --config=ava-js.config.js dist/tests/blockml/apps/blockml/**/*.spec.js"
            ],
            "parallel": false
          }
        },
        "format": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "prettier \"apps/blockml/**/*.{ts,js,json,scss,html}\" --write --config .prettierrc.yaml"
          }
        },
        "circular": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "madge --circular apps/blockml"
          }
        }
      }
    },
    "backend": {
      "root": "apps/backend",
      "sourceRoot": "apps/backend/src",
      "projectType": "application",
      "prefix": "backend",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/backend",
            "main": "apps/backend/src/main.ts",
            "tsConfig": "apps/backend/tsconfig.app.json",
            "assets": ["apps/backend/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "backend:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/backend/**/*.ts"]
          }
        },
        "build-tests": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "rimraf \"./dist/tests/backend\" && tsc --project apps/backend/tsconfig.test.json",
            "outputPath": "dist/tests/backend"
          }
        },
        "e2e": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              "nx build-tests backend --with-deps",
              "ava --config=ava-js-e2e.config.js dist/tests/backend/apps/backend/**/*.e2e-spec.js"
            ],
            "parallel": false
          }
        },
        "format": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "prettier \"apps/backend/**/*.{ts,js,json,scss,html}\" --write --config .prettierrc.yaml"
          }
        },
        "circular": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "madge --circular apps/backend"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "disk"
}
