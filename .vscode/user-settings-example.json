{
  "editor.pasteAs.enabled": false,
  "explorer.compactFolders": false,
  "workbench.settings.useSplitJSON": false,
  "workbench.tree.enableStickyScroll": false,
  "editor.stickyScroll.enabled": false,
  "update.mode": "manual",
  "sql.linter.executablePath": "/usr/local/bin/sqlfluff",
  "[njk]": {
    "editor.defaultFormatter": "douglaszaltron.nunjucks-vscode-extensionpack"
  },
  "cSpell.enabled": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "search.exclude": {
    "**/node_modules": true,
    "**/dist": true
  },
  "workbench.editor.closeOnFileDelete": true,
  "files.enableTrash": false,
  "workbench.colorTheme": "Darcula Extra",
  "diffEditor.ignoreTrimWhitespace": false,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "editor.links": false,
  "terminal.integrated.scrollback": 10000,
  "editor.accessibilitySupport": "off",
  "workbench.iconTheme": "material-icon-theme",
  "editor.suggest.showReferences": false,
  "files.autoSave": "afterDelay",
  "javascript.updateImportsOnFileMove.enabled": "never",
  "workbench.startupEditor": "newUntitledFile",
  "typescript.updateImportsOnFileMove.enabled": "never",
  "extensions.ignoreRecommendations": true,
  "files.autoSaveDelay": 300,
  "editor.largeFileOptimizations": false,
  "workbench.panel.defaultLocation": "bottom",
  "typescript.referencesCodeLens.enabled": true,
  "editor.detectIndentation": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "window.zoomLevel": 3.25,
  "editor.bracketPairColorization.enabled": false,
  "workbench.editor.splitInGroupLayout": "horizontal",
  "editor.tabSize": 2,
  "eslint.validate": ["javascript", "typescript"],
  "explorer.confirmDelete": false,
  "workbench.settings.editor": "json",
  "editor.codeLens": true,
  "security.workspace.trust.untrustedFiles": "open",
  "[python]": {
    "editor.formatOnType": true
  },
  "git.openRepositoryInParentFolders": "never",
  "workbench.editor.empty.hint": "hidden",
  "workbench.activityBar.location": "top",
  "remote.autoForwardPortsSource": "hybrid",
  "workbench.editor.enablePreview": false,
  "editor.pasteAs.preferences": ["text.plain"],

  "diffEditor.maxComputationTime": 0,
  "biome.suggestInstallingGlobally": false,
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one",
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.wordWrap": "off",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  "claudeCode.preferredLocation": "panel",
  "redhat.telemetry.enabled": false,
  "telemetry.telemetryLevel": "off",
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "malloy.telemetry": false
}
