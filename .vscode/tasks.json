{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Ava run spec file - backend / blockml / disk / mcli",
      "command": "./node_modules/.bin/ava",
      "args": ["${relativeFile}"],
      "options": {
        "env": {
          "BLOCKML_LOG_IO": "TRUE"
        }
      },
      "problemMatcher": [],
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "dedicated"
      }
    }
  ]
}
