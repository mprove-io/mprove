{
  "version": "0.2.0",
  "compounds": [
    {
      "name": "App",
      "configurations": [
        "chrome 4200",
        "disk 9230",
        "backend 9231",
        "backend-scheduler 9232",
        "blockml-single 9233",
        "blockml-main 9234",
        "blockml-worker 9235"
      ],
      "stopAll": true
    }
  ],
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Ava file",
      "program": "${workspaceRoot}/node_modules/.bin/ava",
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal",
      "args": ["${relativeFile}"],
      "env": {
        "ENV_FILE_PATH": ".env",
        "BLOCKML_LOG_IO": "TRUE"
      }
    },
    {
      "name": "chrome 4200",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "disk 9230",
      "port": 9230,
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "remoteRoot": "/usr/src/app"
    },
    {
      "name": "backend 9231",
      "port": 9231,
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "remoteRoot": "/usr/src/app"
    },
    {
      "name": "backend-scheduler 9232",
      "port": 9232,
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "remoteRoot": "/usr/src/app"
    },
    {
      "name": "blockml-single 9233",
      "port": 9233,
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "remoteRoot": "/usr/src/app"
    },
    {
      "name": "blockml-main 9234",
      "port": 9234,
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "remoteRoot": "/usr/src/app"
    },
    {
      "name": "blockml-worker 9235",
      "port": 9235,
      "request": "attach",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "remoteRoot": "/usr/src/app"
    }
  ]
}
