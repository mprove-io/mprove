{
  "name": "mprove-blockml",
  "author": "Alexander Kalitenya <akalitenya@mprove.io>",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "circular": "madge --circular --extensions ts src",
    "test": "npm-run-all clean-src-logs && npm run clean-models-logs && npm run lint && npm run circular && jest && npm run clean-src-logs",
    "t": "jest",
    "clean-src-logs": "rimraf \"./src/logs\"",
    "clean-models-logs": "rimraf \"./src/models/**/logs\""
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^1.15.2",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "class-transformer": "0.3.1",
    "class-transformer-validator": "0.9.1",
    "class-validator": "0.12.2",
    "clone-regexp": "^2.2.0",
    "d3-format": "^1.4.5",
    "fs-extra": "^9.0.1",
    "p-iteration": "^1.1.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "tarjan-graph": "^2.0.0",
    "tiny-async-pool": "^1.2.0",
    "toposort": "^2.0.2",
    "typescript": "4.0.3",
    "walk": "^2.3.9"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.10",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.6.3",
    "madge": "3.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.4.4",
    "ts-loader": "^6.2.1",
    "ts-node": "9.1.1",
    "tsconfig-paths": "^3.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "testTimeout": 60000,
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFiles": [
      "./jest-setup-file.ts"
    ]
  }
}
