    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/cli": "11.2.6",
    "@nrwl/eslint-plugin-nx": "11.2.6",
    "@nrwl/jest": "11.2.6",
    "@nrwl/linter": "11.2.6",
    "@nrwl/nest": "^11.2.6",
    "@nrwl/node": "11.2.6",
    "@nrwl/tao": "11.2.6",
    "@nrwl/workspace": "11.2.6",
    "@types/jest": "26.0.8",
    "@types/node": "12.12.38",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.0.0",
    "husky": "1.3.1",
    "jest": "26.2.2",
    "lint-staged": "7.3.0",
    "prettier": "2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "@philidem/rewrite-relative-paths": "^1.1.1",
    "@types/express": "^4.17.3",
    "@types/fs-extra": "^9.0.1",
    "@types/nodegit": "^0.26.10",
    "@types/supertest": "^2.0.8",
    "ava": "^3.15.0",
    "eslint-plugin-import": "^2.20.1",
    "madge": "3.11.0",
    "supertest": "^4.0.2",
    "ts-loader": "^6.2.1",
    "tsconfig-paths": "^3.9.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "./**/*.{ts,js,json,scss,html,md}": [
        "prettier --write --config .prettierrc.yaml",
        "git add"
      ]
    }
  },
  "_moduleAliases": {
    "~blockml": "dist/tests/blockml/apps/blockml/src",
    "~disk": "dist/tests/disk/apps/disk/src",
    "~api": "dist/tests/api",
    "@mprove/api": "dist/tests/api/_index"
  }
}
