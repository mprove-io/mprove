{
  "name": "mprove",
  "version": "11.0.96-dev",
  "author": "Alexander Kalitenya <akalitenya@mprove.io>",
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.27.0",
  "private": true,
  "sideEffects": [
    "apps/front/src/polyfills.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mprove-io/mprove.git"
  },
  "scripts": {
    "check:db": "node -r ts-node/register -r tsconfig-paths/register apps/backend/src/wait-for-db.ts",
    "mig:custom": "drizzle-kit generate --custom --config apps/backend/src/drizzle/postgres/drizzle.config.ts --name=$(date -u +\"%Y-%m-%d_%H-%M-%S\")",
    "mig:gen": "drizzle-kit generate --config apps/backend/src/drizzle/postgres/drizzle.config.ts --name=$(date -u +\"%Y-%m-%d_%H-%M-%S\")",
    "mig:run": "pnpm node -r ts-node/register -r tsconfig-paths/register apps/backend/src/drizzle/postgres/drizzle-migrate.ts",
    "mig:dropmig": "drizzle-kit drop --config apps/backend/src/drizzle/postgres/drizzle.config.ts",
    "dz:pull": "drizzle-kit introspect --config apps/backend/src/drizzle/postgres/drizzle.config.ts",
    "dz:push": "drizzle-kit push --config apps/backend/src/drizzle/postgres/drizzle.config.ts",
    "dz:upmeta": "drizzle-kit up --config apps/backend/src/drizzle/postgres/drizzle.config.ts",
    "dz:check": "drizzle-kit check --config apps/backend/src/drizzle/postgres/drizzle.config.ts",
    "mcli": "ENV_FILE_PATH=.env pnpm node -r ts-node/register -r tsconfig-paths/register apps/mcli/src/main.ts",
    "lint": "biome lint",
    "bfm": "biome check --write",
    "pfm": "prettier \"**/*.{html,scss,json,jsonc,md}\" --write --config .prettierrc.js",
    "circular": "turbo run circular",
    "circular:disk": "turbo run circular --filter=@mprove/disk",
    "circular:front": "turbo run circular --filter=@mprove/front",
    "build:prod": "turbo run build:prod",
    "build:backend:prod": "turbo run build:prod --filter=@mprove/backend",
    "build:blockml:prod": "turbo run build:prod --filter=@mprove/blockml",
    "build:chat:prod": "turbo run build:prod --filter=@mprove/chat",
    "build:disk:prod": "turbo run build:prod --filter=@mprove/disk",
    "build:front:prod": "turbo run build:prod --filter=@mprove/front",
    "build:mcli": "rm -rf apps/mcli/dist/* && turbo run build:prod --filter=@mprove/mcli",
    "serve": "turbo run serve",
    "serve:backend": "turbo run serve --filter=@mprove/backend",
    "serve:blockml": "turbo run serve --filter=@mprove/blockml",
    "serve:chat": "turbo run serve --filter=@mprove/chat",
    "serve:disk": "turbo run serve --filter=@mprove/disk",
    "serve:front": "turbo run serve --filter=@mprove/front",
    "start": "turbo run start",
    "start:backend": "turbo run start --filter=@mprove/backend",
    "start:blockml": "turbo run start --filter=@mprove/blockml",
    "start:chat": "turbo run start --filter=@mprove/chat",
    "start:disk": "turbo run start --filter=@mprove/disk",
    "start:front": "turbo run start --filter=@mprove/front",
    "debug:disk": "turbo run debug --filter=@mprove/disk",
    "test": "turbo run test",
    "test:disk": "turbo run test --filter=@mprove/disk",
    "test:blockml": "IS_TELEMETRY_ENABLED=FALSE turbo run test --filter=@mprove/blockml",
    "e2e": "turbo run e2e",
    "e2e:backend": "IS_TELEMETRY_ENABLED=FALSE turbo run e2e --filter=@mprove/backend",
    "e2e:mcli": "IS_TELEMETRY_ENABLED=FALSE pnpm clean:mcli-repos && pnpm clone:mcli && turbo run e2e --filter=@mprove/mcli",
    "clone:mcli": "git clone $MPROVE_CLI_TEST_REMOTE_GIT_URL $MPROVE_CLI_TEST_DESTINATION_URL",
    "make:dist": "mkdir -p apps/disk/dist apps/backend/dist apps/blockml/dist apps/chat/dist apps/front/dist apps/mcli/dist libs/common/dist libs/node-common/dist",
    "clean": "pnpm clean:mprove_data && pnpm clean:dist && pnpm clean:angular && pnpm clean:turbo",
    "clean:mprove_data": "rm -rf mprove_data && mkdir -p mprove_data mprove_data/blockml-data mprove_data/blockml-logs mprove_data/db-main mprove_data/dwh-mysql mprove_data/dwh-postgres mprove_data/mcli-repos mprove_data/organizations mprove_data/clickstack-db mprove_data/clickstack-ch-data mprove_data/clickstack-ch-logs",
    "clean:opencode": "rm -rf mprove_data/opencode-cache/* && rm -rf mprove_data/opencode-config/* && rm -rf mprove_data/opencode-state/* && rm -rf mprove_data/opencode-share/*",
    "clean:clickstack": "rm -rf mprove_data/clickstack-db/* && rm -rf mprove_data/clickstack-ch-data/* && rm -rf mprove_data/clickstack-ch-logs/*",
    "clean:clickstack-ch": "rm -rf mprove_data/clickstack-ch-data/* && rm -rf mprove_data/clickstack-ch-logs/*",
    "clean:app": "pnpm clean:blockml-data && pnpm clean:blockml-logs && pnpm clean:clickstack-ch-data && pnpm clean:clickstack-ch-logs && pnpm clean:db-main && pnpm clean:dwh-mysql && pnpm clean:dwh-postgres && pnpm clean:mcli-repos && pnpm clean:organizations",
    "clean:blockml-data": "rm -rf mprove_data/blockml-data/*",
    "clean:blockml-logs": "rm -rf mprove_data/blockml-logs/*",
    "clean:clickstack-ch-data": "rm -rf mprove_data/clickstack-ch-data/*",
    "clean:clickstack-ch-logs": "rm -rf mprove_data/clickstack-ch-logs/*",
    "clean:db-main": "rm -rf mprove_data/db-main/*",
    "clean:dwh-mysql": "rm -rf mprove_data/dwh-mysql/*",
    "clean:dwh-postgres": "rm -rf mprove_data/dwh-postgres/*",
    "clean:mcli-repos": "rm -rf mprove_data/mcli-repos/*",
    "clean:organizations": "rm -rf mprove_data/organizations/*",
    "clean:dist": "pnpm make:dist && rm -rf apps/disk/dist/* apps/mcli/dist/* apps/backend/dist/* apps/blockml/dist/* apps/chat/dist/* apps/front/dist/* libs/common/dist/* libs/node-common/dist/*",
    "clean:angular": "rm -rf .angular/*",
    "clean:turbo": "rm -rf .turbo/*",
    "clean:deps": "pnpm clean:pnpm-store && pnpm clean:node_modules && pnpm clean:backend:node_modules && pnpm clean:blockml:node_modules && pnpm clean:chat:node_modules && pnpm clean:disk:node_modules && pnpm clean:front:node_modules && pnpm clean:mcli:node_modules",
    "clean:pnpm-store": "rm -rf ./.pnpm-store/*",
    "clean:node_modules": "rm -rf ./node_modules/* && rm -rf ./node_modules/.bin/* && rm -rf ./node_modules/.cache/* && rm -rf ./node_modules/.pnpm/* && rm -rf ./node_modules/.tmp/* && rm -rf ./node_modules/.modules.yaml && rm -rf ./node_modules/.pnpm-workspace-state-v1.json",
    "clean:backend:node_modules": "rm -rf apps/backend/node_modules/* && rm -rf apps/backend/node_modules/.bin/*",
    "clean:blockml:node_modules": "rm -rf apps/blockml/node_modules/* && rm -rf apps/blockml/node_modules/.bin/*",
    "clean:chat:node_modules": "rm -rf apps/chat/node_modules/* && rm -rf apps/chat/node_modules/.bin/*",
    "clean:disk:node_modules": "rm -rf apps/disk/node_modules/* && rm -rf apps/disk/node_modules/.bin/*",
    "clean:front:node_modules": "rm -rf apps/front/node_modules/* && rm -rf apps/front/node_modules/.bin/*",
    "clean:mcli:node_modules": "rm -rf apps/mcli/node_modules/* && rm -rf apps/mcli/node_modules/.bin/*",
    "clean:npm": "npm cache clean --force",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@types/node": "catalog:",
    "ava": "catalog:",
    "cross-env": "catalog:",
    "drizzle-kit": "catalog:",
    "esbuild": "catalog:",
    "husky": "catalog:",
    "license-checker": "catalog:",
    "lint-staged": "catalog:",
    "madge": "catalog:",
    "prettier": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:",
    "rimraf": "catalog:",
    "shx": "catalog:",
    "ts-node": "catalog:",
    "tsconfig-paths": "catalog:",
    "turbo": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:"
  },
  "madge": {
    "fileExtensions": [
      "ts",
      "tsx"
    ],
    "tsConfig": "./tsconfig.json"
  },
  "lint-staged": {
    "./**/*.{css,ts,js}": [
      "biome check --write"
    ],
    "./**/*.{html,scss,json,jsonc,md}": [
      "prettier --write --config .prettierrc.js"
    ]
  },
  "vaadin": {
    "disableUsageStatistics": true
  },
  "pnpm": {
    "overrides": {
      "rxjs": "catalog:"
    }
  }
}
