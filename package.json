{
  "name": "mprove",
  "author": "Alexander Kalitenya <akalitenya@mprove.io>",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mprove-io/mprove.git"
  },
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "clean-blockml-logs": "rimraf \"./apps/blockml/src/models/**/logs\"",
    "t:api": "rimraf \"./dist/tests/api\" && tsc --project tsconfig-api.test.json",
    "t:backend": "rimraf \"./dist/tests/backend\" && tsc --project tsconfig-backend.test.json",
    "t:blockml": "rimraf \"./dist/tests/blockml\" && rimraf \"./apps/blockml/src/models/**/logs\" && tsc --project tsconfig-blockml.test.json",
    "t:disk": "rimraf \"./dist/tests/disk\" && tsc --project tsconfig-disk.test.json",
    "t:libs": "npm run t:api",
    "t:apps": "npm run t:backend && npm run t:blockml && npm run t:disk",
    "t:all": "npm run t:libs && npm run t:apps",
    "test:backend": "npm run t:libs && npm run t:backend && ava --config=ava-js.config.js dist/tests/backend/apps/**/*.e2e-spec.js",
    "test:blockml": "npm run t:libs && npm run t:blockml && ava --config=ava-js.config.js dist/tests/blockml/apps/**/*.spec.js",
    "test:disk": "npm run t:libs && npm run t:disk && ava --config=ava-js.config.js dist/tests/disk/apps/**/*.spec.js",
    "test:all": "npm run test:backend && npm run test:disk && npm run test:blockml",
    "circular": "madge --circular {apps,libs}",
    "lint:all": "eslint \"{apps,libs}/**/*.ts\"",
    "typeorm": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "p:api": "prettier \"libs/api/**/*.{ts,js,json,scss,html}\" --write --config .prettierrc.yaml",
    "p:backend": "prettier \"apps/backend/**/*.{ts,js,json,scss,html}\" --write --config .prettierrc.yaml",
    "p:blockml": "prettier \"apps/blockml/**/*.{ts,js,json,scss,html}\" --write --config .prettierrc.yaml",
    "p:disk": "prettier \"apps/disk/**/*.{ts,js,json,scss,html}\" --write --config .prettierrc.yaml",
    "p:all": "prettier \"**/*.{ts,js,json,scss,html}\" --write --config .prettierrc.yaml"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "@golevelup/nestjs-rabbitmq": "^1.15.2",
    "@nestjs/config": "^0.6.1",
    "class-transformer": "0.3.1",
    "class-transformer-validator": "0.9.1",
    "class-validator": "0.12.2",
    "clone-regexp": "^2.2.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^9.0.1",
    "module-alias": "^2.2.2",
    "nodegit": "^0.27.0",
    "p-iteration": "^1.1.8",
    "rimraf": "^3.0.2",
    "typescript": "~4.0.3",
    "d3-format": "^1.4.5",
    "tarjan-graph": "^2.0.0",
    "tiny-async-pool": "^1.2.0",
    "toposort": "^2.0.2",
    "walk": "^2.3.9",
    "@nestjs-modules/mailer": "^1.5.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/typeorm": "^7.1.5",
    "bcrypt": "^5.0.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.4.17",
    "nodemailer-mailgun-transport": "1.4.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/cli": "11.2.6",
    "@nrwl/eslint-plugin-nx": "11.2.6",
    "@nrwl/jest": "11.2.6",
    "@nrwl/linter": "11.2.6",
    "@nrwl/nest": "^11.2.6",
    "@nrwl/node": "11.2.6",
    "@nrwl/tao": "11.2.6",
    "@nrwl/workspace": "11.2.6",
    "@types/jest": "26.0.8",
    "@types/node": "12.12.38",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.0.0",
    "husky": "1.3.1",
    "jest": "26.2.2",
    "lint-staged": "7.3.0",
    "prettier": "2.2.1",
    "prettier-plugin-organize-imports": "^1.1.1",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "@philidem/rewrite-relative-paths": "^1.1.1",
    "@types/express": "^4.17.3",
    "@types/fs-extra": "^9.0.1",
    "@types/nodegit": "^0.26.10",
    "@types/supertest": "^2.0.8",
    "ava": "^3.15.0",
    "eslint-plugin-import": "^2.20.1",
    "madge": "4.0.0",
    "supertest": "^4.0.2",
    "ts-loader": "^6.2.1",
    "tsconfig-paths": "^3.9.0",
    "@types/bcrypt": "^3.0.0"
  },
  "madge": {
    "fileExtensions": [
      "ts",
      "tsx"
    ],
    "tsConfig": "./tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "./**/*.{ts,js,json,scss,html,md}": [
        "prettier --write --config .prettierrc.yaml",
        "git add"
      ]
    }
  },
  "_moduleAliases": {
    "~backend": "dist/tests/backend/apps/backend/src",
    "~blockml": "dist/tests/blockml/apps/blockml/src",
    "~disk": "dist/tests/disk/apps/disk/src",
    "~api": "dist/tests/api",
    "@mprove/api": "dist/tests/api/_index"
  }
}
