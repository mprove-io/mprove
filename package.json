{
  "name": "mprove",
  "version": "8.2.1",
  "author": "Alexander Kalitenya <akalitenya@mprove.io>",
  "license": "Apache-2.0",
  "private": false,
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/mprove-io/mprove.git"
  },
  "bin": "./main.js",
  "scripts": {
    "postinstall": "yarn pj",
    "mcli": "ENV_FILE_PATH=.env yarn node -r ts-node/register -r tsconfig-paths/register apps/mcli/src/main.ts",
    "typeorm": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "mig:drop": "npm run typeorm schema:drop -- -d apps/backend/src/typeorm-datasource.ts",
    "mig:run": "npm run typeorm migration:run -- -d apps/backend/src/typeorm-datasource.ts",
    "mig:gen": "npm run typeorm migration:generate apps/backend/src/migrations/All -- -d apps/backend/src/typeorm-datasource.ts",
    "mig": "npm run mig:drop && npm run mig:run && npm run mig:gen && npm run mig:run",
    "fm": "prettier \"**/*.{ts,js,json,scss,html,md}\" --write --config .prettierrc.js",
    "circular": "madge --circular apps libs",
    "lint": "eslint \"**/*.ts\"",
    "pj": "yarn pjc && yarn pjd",
    "pjc": "ts-node ./tools/utils/prepare-package-json-for-cli",
    "pjd": "ts-node ./tools/utils/prepare-package-json-for-docker",
    "pjc-to-dist": "shx cp package.cli.json dist/apps/mcli/package.json && shx cp yarn.lock dist/apps/mcli/yarn.lock",
    "pjc-to-tests": "shx cp package.cli.json dist/tests/mcli/package.json",
    "publish-cli": "cd dist/apps/mcli && yarn publish",
    "compose-build": "yarn pj && docker-compose build",
    "nx": "nx",
    "build:mcli": "rm -rf dist/apps/mcli/* && nx build mcli && yarn pjc-to-dist && yarn add-line:mcli",
    "build:mcli:win": "rm -rf dist/apps/mcli/* && nx build mcli && yarn pjc-to-dist",
    "add-line:mcli": "(shx echo '#!/usr/bin/env node' && shx cat dist/apps/mcli/main.js) > tempfile && shx mv tempfile dist/apps/mcli/main.js",
    "build-tests:mcli": "nx build-tests mcli && yarn pjc-to-tests",
    "ava:mcli": "yarn clean:mcli-repos && yarn clone:mcli && ava --config=ava-js.config.js dist/tests/mcli/apps/mcli/**/*.spec.js",
    "clone:mcli": "git clone $MPROVE_CLI_TEST_REMOTE_GIT_URL $MPROVE_CLI_TEST_DESTINATION_URL",
    "build:prod": "nx run-many --target=build -c production --all",
    "build:backend:prod": "nx build backend -c production",
    "build:blockml:prod": "nx build blockml -c production",
    "build:disk:prod": "nx build disk -c production",
    "build:front:prod": "nx build front -c production",
    "serve:disk": "nx serve disk",
    "serve:disk:9230": "nx serve disk --port 9230",
    "serve:backend": "nx serve backend",
    "serve:backend:9231": "nx serve backend --port 9231",
    "serve:backend:scheduler": "LISTEN_PORT=3032 BACKEND_IS_SCHEDULER=TRUE nx serve backend",
    "serve:backend:scheduler:9232": "LISTEN_PORT=3032 BACKEND_IS_SCHEDULER=TRUE nx serve backend --port 9232",
    "serve:backend:combo": "BACKEND_IS_SCHEDULER=TRUE nx serve backend",
    "serve:blockml": "nx serve blockml",
    "serve:blockml:single:9233": "BLOCKML_IS_SINGLE=TRUE nx serve blockml --port 9233",
    "serve:blockml:main:9234": "LISTEN_PORT=3034 BLOCKML_IS_SINGLE=FALSE BLOCKML_IS_MAIN=TRUE nx serve blockml --port 9234",
    "serve:blockml:worker:9235": "LISTEN_PORT=3035 BLOCKML_IS_SINGLE=FALSE BLOCKML_IS_WORKER=TRUE nx serve blockml --port 9235",
    "serve:blockml:main-worker:9236": "LISTEN_PORT=3036 BLOCKML_IS_SINGLE=FALSE BLOCKML_IS_MAIN=TRUE BLOCKML_IS_WORKER=TRUE nx serve blockml --port 9236",
    "serve:front:dev": "nx serve front -c development",
    "serve:front:dev:host": "nx serve front -c development --host 0.0.0.0 --disable-host-check",
    "test": "nx run-many --target=test --all",
    "test:mcli": "yarn clean:mcli-repos && yarn clone:mcli && nx test mcli --verbose",
    "test:disk": "nx test disk --verbose",
    "test:blockml": "nx test blockml --verbose",
    "e2e": "nx run-many --target=e2e --all",
    "e2e:backend": "nx e2e backend",
    "e2e:integra": "nx e2e integra",
    "make:dist": "mkdir -p dist/apps/disk dist/apps/backend dist/apps/blockml dist/apps/front",
    "clean": "yarn clean:db && yarn clean:org && yarn clean:dist && yarn clean:pg && yarn clean:ch && yarn clean:blockml-logs && yarn clean:mcli-repos && yarn clean:angular && yarn clean:nx && yarn clean:cypress",
    "clean:org-db": "yarn clean:db && yarn clean:org",
    "clean:db": "rm -rf ~/mprove_data/mysql/*",
    "clean:org": "rm -rf ~/mprove_data/organizations/*",
    "clean:dist": "yarn make:dist && rm -rf dist/apps/disk/* dist/apps/mcli/* dist/apps/backend/* dist/apps/blockml/* dist/apps/front/* dist/tests/* dist/bin/*",
    "clean:pg": "rm -rf ~/mprove_data/dwh-postgres/*",
    "clean:ch": "rm -rf ~/mprove_data/dwh-clickhouse/* ~/mprove_data/dwh-clickhouse-logs/*",
    "clean:blockml-logs": "rm -rf ~/mprove_data/blockml-logs/*",
    "clean:mcli-repos": "rm -rf ~/mprove_data/mcli-repos/*",
    "clean:angular": "rm -rf .angular/*",
    "clean:nx": "rm -rf ./node_modules/.cache/nx",
    "clean:cypress": "rm -rf ~/.cache/Cypress/*",
    "clean:node_modules": "rm -rf ./node_modules/*",
    "clean:npm": "npm cache clean --force",
    "x11": "docker-compose up -d --force-recreate x11-bridge",
    "pacx:mcli": "caxa --input dist/apps/mcli --output dist/bin/mp -- \"{{caxa}}/node_modules/.bin/node\" \"{{caxa}}/main.js\"",
    "pacx:mcli:win": "caxa --input dist/apps/mcli --output dist/bin/mp.exe -- \"{{caxa}}/node_modules/.bin/node\" \"{{caxa}}/main.js\""
  },
