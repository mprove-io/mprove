{
  "name": "mprove",
  "version": "11.0.100-dev",
  "author": "Alexander Kalitenya <akalitenya@mprove.io>",
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.29.3",
  "private": true,
  "sideEffects": [
    "apps/front/src/polyfills.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mprove-io/mprove.git"
  },
  "scripts": {
    "prepare": "husky",
    "version-write": "bash scripts/dev/version-write.sh",
    "catalog-write": "bash scripts/dev/catalog-write.sh",
    "deps-write": "bash scripts/dev/deps-write.sh",
    "deps-check": "bash scripts/dev/deps-check.sh",
    "get-shards": "pnpm exec tsx scripts/dev/get-shards.ts",
    "create-light-plus-ext": "pnpm exec tsx scripts/dev/create-light-plus-ext.ts",
    "biome-check": "biome check .",
    "biome-write": "biome check --write .",
    "prettier-check": "prettier \"**/*.{html,scss,json,jsonc,md}\" --check --config .prettierrc.js",
    "prettier-write": "prettier \"**/*.{html,scss,json,jsonc,md}\" --write --config .prettierrc.js",
    "check": "pnpm typecheck && pnpm lint",
    "typecheck": "pnpm typecheck:turbo && pnpm typecheck:mcli && pnpm typecheck:scripts",
    "lint": "pnpm lint:turbo && pnpm lint:mcli && pnpm lint:scripts",
    "circular": "pnpm circular:turbo && pnpm circular:mcli && pnpm circular:scripts",
    "build": "pnpm build:turbo && pnpm build:mcli",
    "start": "turbo run start",
    "debug": "turbo run debug",
    "test": "turbo run test",
    "e2e": "pnpm e2e:turbo && pnpm e2e:mcli",
    "inst": "pnpm catalog-write && pnpm install && pnpm install:mcli",
    "install:mcli": "cd mcli && bun install",
    "audit:nofail": "pnpm audit:apps || true && pnpm audit:mcli || true",
    "audit:apps": "pnpm audit",
    "audit:mcli": "cd mcli && NODE_EXTRA_CA_CERTS=/etc/ssl/certs/ca-certificates.crt bun audit",
    "build:mcli:linux": "cd mcli && bun run build:linux",
    "mcli": "cd mcli && bun run src/main.ts",
    "prep:mcli": "pnpm clean:mcli-repos && git clone $MPROVE_CLI_TEST_REMOTE_GIT_URL $MPROVE_CLI_TEST_DESTINATION_URL",
    "check:db": "cd apps/backend && node --import @swc-node/register/esm-register src/wait-for-db.ts",
    "mig:custom": "cd apps/backend && drizzle-kit generate --custom --config src/drizzle/postgres/drizzle.config.ts --name=$(date -u +\"%Y-%m-%d_%H-%M-%S\")",
    "mig:gen": "cd apps/backend && drizzle-kit generate --config src/drizzle/postgres/drizzle.config.ts --name=$(date -u +\"%Y-%m-%d_%H-%M-%S\")",
    "mig:run": "cd apps/backend && node --import @swc-node/register/esm-register src/drizzle/postgres/drizzle-migrate.ts",
    "mig:dropmig": "cd apps/backend && drizzle-kit drop --config src/drizzle/postgres/drizzle.config.ts",
    "dz-kit:pull": "cd apps/backend && drizzle-kit introspect --config src/drizzle/postgres/drizzle.config.ts",
    "dz-kit:push": "cd apps/backend && drizzle-kit push --config src/drizzle/postgres/drizzle.config.ts",
    "dz-kit:upmeta": "cd apps/backend && drizzle-kit up --config src/drizzle/postgres/drizzle.config.ts",
    "dz-kit:check": "cd apps/backend && drizzle-kit check --config src/drizzle/postgres/drizzle.config.ts",
    "check:turbo": "turbo run check",
    "check:backend": "turbo run check --filter=@mprove/backend",
    "check:blockml": "turbo run check --filter=@mprove/blockml",
    "check:disk": "turbo run check --filter=@mprove/disk",
    "check:front": "turbo run check --filter=@mprove/front",
    "check:common": "turbo run check --filter=@mprove/common",
    "check:node-common": "turbo run check --filter=@mprove/node-common",
    "check:mcli": "cd mcli && bun run check",
    "check:scripts": "pnpm typecheck:scripts && pnpm lint:scripts",
    "typecheck:turbo": "turbo run typecheck",
    "typecheck:backend": "turbo run typecheck --filter=@mprove/backend",
    "typecheck:blockml": "turbo run typecheck --filter=@mprove/blockml",
    "typecheck:disk": "turbo run typecheck --filter=@mprove/disk",
    "typecheck:front": "turbo run typecheck --filter=@mprove/front",
    "typecheck:common": "turbo run typecheck --filter=@mprove/common",
    "typecheck:node-common": "turbo run typecheck --filter=@mprove/node-common",
    "typecheck:mcli": "cd mcli && bun run typecheck",
    "typecheck:scripts": "tsc --noEmit",
    "lint:turbo": "turbo run lint",
    "lint:backend": "turbo run lint --filter=@mprove/backend",
    "lint:blockml": "turbo run lint --filter=@mprove/blockml",
    "lint:disk": "turbo run lint --filter=@mprove/disk",
    "lint:front": "turbo run lint --filter=@mprove/front",
    "lint:common": "turbo run lint --filter=@mprove/common",
    "lint:node-common": "turbo run lint --filter=@mprove/node-common",
    "lint:mcli": "cd mcli && bun run lint",
    "lint:scripts": "biome lint scripts",
    "circular:turbo": "turbo run circular",
    "circular:backend": "turbo run circular --filter=@mprove/backend",
    "circular:blockml": "turbo run circular --filter=@mprove/blockml",
    "circular:disk": "turbo run circular --filter=@mprove/disk",
    "circular:front": "turbo run circular --filter=@mprove/front",
    "circular:common": "turbo run circular --filter=@mprove/common",
    "circular:node-common": "turbo run circular --filter=@mprove/node-common",
    "circular:mcli": "cd mcli && bun run circular",
    "circular:scripts": "madge --circular scripts",
    "build:turbo": "turbo run build",
    "build:backend": "turbo run build --filter=@mprove/backend",
    "build:blockml": "turbo run build --filter=@mprove/blockml",
    "build:disk": "turbo run build --filter=@mprove/disk",
    "build:front": "turbo run build --filter=@mprove/front",
    "build:mcli": "cd mcli && bun run build",
    "node-serve": "turbo run node-serve",
    "node-serve:backend": "turbo run node-serve --filter=@mprove/backend --log-prefix=none",
    "node-serve:blockml": "turbo run node-serve --filter=@mprove/blockml --log-prefix=none",
    "node-serve:disk": "turbo run node-serve --filter=@mprove/disk --log-prefix=none",
    "node-serve:front": "turbo run node-serve --filter=@mprove/front --log-prefix=none",
    "start:turbo": "turbo run start",
    "start:backend": "turbo run start --filter=@mprove/backend --log-prefix=none",
    "start:blockml": "turbo run start --filter=@mprove/blockml --log-prefix=none",
    "start:disk": "turbo run start --filter=@mprove/disk --log-prefix=none",
    "start:front": "turbo run start --filter=@mprove/front --log-prefix=none",
    "debug:turbo": "turbo run debug",
    "debug:disk": "turbo run debug --filter=@mprove/disk",
    "debug:blockml": "turbo run debug --filter=@mprove/blockml",
    "test:turbo": "turbo run test",
    "test:disk": "turbo run test --filter=@mprove/disk --log-prefix=none",
    "test:blockml": "turbo run test --filter=@mprove/blockml --log-prefix=none",
    "e2e:turbo": "turbo run e2e",
    "e2e:backend": "turbo run e2e --filter=@mprove/backend --log-prefix=none",
    "e2e:mcli": "pnpm prep:mcli && cd mcli && bun run e2e",
    "e2x": "pnpm e2x:turbo",
    "e2x:turbo": "turbo run e2x",
    "e2x:backend": "turbo run e2x --filter=@mprove/backend --log-prefix=none",
    "custom:backend": "turbo run custom --filter=@mprove/backend --log-prefix=none",
    "e2b:build": "node --import @swc-node/register/esm-register scripts/e2b/build-template.ts",
    "e2b:templates": "node --import @swc-node/register/esm-register scripts/e2b/list-templates.ts",
    "clean:app": "pnpm clean:blockml-data && pnpm clean:blockml-logs && pnpm clean:clickstack-ch-data && pnpm clean:clickstack-ch-logs && pnpm clean:db-main && pnpm clean:dwh-mysql && pnpm clean:dwh-postgres && pnpm clean:mcli-repos && pnpm clean:organizations",
    "clean:mprove_data": "pnpm clean:blockml-data && pnpm clean:blockml-logs && pnpm clean:clickstack-ch-data && pnpm clean:clickstack-ch-logs && pnpm clean:clickstack-db && pnpm clean:db-main && pnpm clean:dwh-mysql && pnpm clean:dwh-postgres && pnpm clean:mcli-repos && pnpm clean:organizations",
    "clean:blockml-data": "rimraf --glob \"mprove_data/blockml-data/*\" \"mprove_data/blockml-data/.[!.]*\"",
    "clean:blockml-logs": "rimraf --glob \"mprove_data/blockml-logs/*\" \"mprove_data/blockml-logs/.[!.]*\"",
    "clean:clickstack-ch-data": "rimraf --glob \"mprove_data/clickstack-ch-data/*\" \"mprove_data/clickstack-ch-data/.[!.]*\"",
    "clean:clickstack-ch-logs": "rimraf --glob \"mprove_data/clickstack-ch-logs/*\" \"mprove_data/clickstack-ch-logs/.[!.]*\"",
    "clean:clickstack-db": "rimraf --glob \"mprove_data/clickstack-db/*\" \"mprove_data/clickstack-db/.[!.]*\"",
    "clean:db-main": "rimraf --glob \"mprove_data/db-main/*\" \"mprove_data/db-main/.[!.]*\"",
    "clean:dwh-mysql": "rimraf --glob \"mprove_data/dwh-mysql/*\" \"mprove_data/dwh-mysql/.[!.]*\"",
    "clean:dwh-postgres": "rimraf --glob \"mprove_data/dwh-postgres/*\" \"mprove_data/dwh-postgres/.[!.]*\"",
    "clean:mcli-repos": "rimraf --glob \"mprove_data/mcli-repos/*\" \"mprove_data/mcli-repos/.[!.]*\"",
    "clean:organizations": "rimraf --glob \"mprove_data/organizations/*\" \"mprove_data/organizations/.[!.]*\"",
    "clean:clickstack": "pnpm clean:clickstack-db && pnpm clean:clickstack-ch",
    "clean:clickstack-ch": "pnpm clean:clickstack-ch-data && pnpm clean:clickstack-ch-logs",
    "clean": "pnpm clean:mcli && pnpm clean:nested && pnpm clean:top",
    "clean:mcli": "pnpm clean-bun:mcli && pnpm clean-dist:mcli && pnpm clean-node:mcli",
    "clean-bun:mcli": "rimraf --glob \"mcli/bun.lock\" \"mcli/.bun/*\" \"mcli/.bun/.[!.]*\"",
    "clean-dist:mcli": "rimraf --glob \"mcli/dist/*\" \"mcli/dist/.[!.]*\"",
    "clean-node:mcli": "rimraf --glob \"mcli/node_modules/*\" \"mcli/node_modules/.[!.]*\"",
    "clean:nested": "pnpm clean-turbo && pnpm clean-dist && pnpm clean-node",
    "clean-turbo": "turbo run clean-turbo",
    "clean-dist": "pnpm clean-dist:mcli && turbo run clean-dist",
    "clean-node": "pnpm clean-node:mcli && turbo run clean-node",
    "clean:top": "pnpm clean:top:turbo && pnpm clean:top:pnpm-store && pnpm clean:top:node",
    "clean:top:turbo": "rimraf --glob \".turbo/*\" \".turbo/.[!.]*\"",
    "clean:top:pnpm-store": "rimraf --glob \".pnpm-store/*\" \".pnpm-store/.[!.]*\"",
    "clean:top:node": "rimraf --glob \"node_modules/*\" \"node_modules/.[!.]*\"",
    "clean:npm": "npm cache clean --force"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@swc-node/register": "catalog:",
    "@swc/core": "catalog:",
    "@types/js-yaml": "catalog:",
    "@types/node": "catalog:",
    "ava": "catalog:",
    "cross-env": "catalog:",
    "dayjs": "catalog:",
    "drizzle-kit": "catalog:",
    "e2b": "catalog:",
    "esbuild": "catalog:",
    "husky": "catalog:",
    "js-yaml": "catalog:",
    "license-checker": "catalog:",
    "lint-staged": "catalog:",
    "madge": "catalog:",
    "prettier": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:",
    "rimraf": "catalog:",
    "shx": "catalog:",
    "tsx": "4.21.0",
    "turbo": "catalog:",
    "typescript": "catalog:"
  },
  "madge": {
    "fileExtensions": [
      "ts",
      "tsx"
    ],
    "tsConfig": "./tsconfig.json"
  },
  "lint-staged": {
    "./**/*.{css,ts,js}": [
      "biome check --write"
    ],
    "./**/*.{html,scss,json,jsonc,md}": [
      "prettier --write --config .prettierrc.js"
    ]
  },
  "vaadin": {
    "disableUsageStatistics": true
  },
  "pnpm": {
    "overrides": {
      "rxjs": "catalog:"
    },
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "@nestjs/core",
      "@parcel/watcher",
      "@sandbox-agent/cli-linux-x64",
      "@swc/core",
      "bcrypt",
      "duckdb",
      "esbuild",
      "husky",
      "isolated-vm",
      "lmdb",
      "msgpackr-extract",
      "protobufjs"
    ]
  }
}
