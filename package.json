{
  "name": "mprove",
  "version": "11.0.96-dev",
  "author": "Alexander Kalitenya <akalitenya@mprove.io>",
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.27.0",
  "private": true,
  "sideEffects": [
    "apps/front/src/polyfills.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mprove-io/mprove.git"
  },
  "scripts": {
    "prepare": "husky",
    "check:db": "node -r ts-node/register -r tsconfig-paths/register apps/backend/src/wait-for-db.ts",
    "mig:custom": "drizzle-kit generate --custom --config apps/backend/src/drizzle/postgres/drizzle.config.ts --name=$(date -u +\"%Y-%m-%d_%H-%M-%S\")",
    "mig:gen": "drizzle-kit generate --config apps/backend/src/drizzle/postgres/drizzle.config.ts --name=$(date -u +\"%Y-%m-%d_%H-%M-%S\")",
    "mig:run": "pnpm node -r ts-node/register -r tsconfig-paths/register apps/backend/src/drizzle/postgres/drizzle-migrate.ts",
    "mig:dropmig": "drizzle-kit drop --config apps/backend/src/drizzle/postgres/drizzle.config.ts",
    "dz:pull": "drizzle-kit introspect --config apps/backend/src/drizzle/postgres/drizzle.config.ts",
    "dz:push": "drizzle-kit push --config apps/backend/src/drizzle/postgres/drizzle.config.ts",
    "dz:upmeta": "drizzle-kit up --config apps/backend/src/drizzle/postgres/drizzle.config.ts",
    "dz:check": "drizzle-kit check --config apps/backend/src/drizzle/postgres/drizzle.config.ts",
    "mcli": "pnpm node -r ts-node/register -r tsconfig-paths/register apps/mcli/src/main.ts",
    "pkg:mcli": "turbo run pack --filter=@mprove/mcli",
    "pkg:mcli:linux": "turbo run pack --filter=@mprove/mcli -- --no-bytecode --public-packages '*' --public --targets node24-linux-x64",
    "clone:mcli": "git clone $MPROVE_CLI_TEST_REMOTE_GIT_URL $MPROVE_CLI_TEST_DESTINATION_URL",
    "biome-check": "biome check \"./**/*.{css,ts,js}\"",
    "biome-write": "biome check --write \"./**/*.{css,ts,js}\"",
    "prettier-check": "prettier \"**/*.{html,scss,json,jsonc,md}\" --check --config .prettierrc.js",
    "prettier-write": "prettier \"**/*.{html,scss,json,jsonc,md}\" --write --config .prettierrc.js",
    "make:dist": "mkdir -p apps/backend/dist apps/blockml/dist apps/chat/dist apps/disk/dist apps/front/dist apps/mcli/dist",
    "lint": "turbo run lint",
    "circular": "turbo run circular",
    "build:prod": "turbo run build:prod",
    "start": "turbo run start",
    "debug": "turbo run debug",
    "test": "turbo run test",
    "e2e": "turbo run e2e",
    "lint:backend": "turbo run lint --filter=@mprove/backend",
    "lint:blockml": "turbo run lint --filter=@mprove/blockml",
    "lint:chat": "turbo run lint --filter=@mprove/chat",
    "lint:disk": "turbo run lint --filter=@mprove/disk",
    "lint:mcli": "turbo run lint --filter=@mprove/mcli",
    "lint:front": "turbo run lint --filter=@mprove/front",
    "lint:common": "turbo run lint --filter=@mprove/common",
    "lint:node-common": "turbo run lint --filter=@mprove/node-common",
    "circular:backend": "turbo run circular --filter=@mprove/backend",
    "circular:blockml": "turbo run circular --filter=@mprove/blockml",
    "circular:chat": "turbo run circular --filter=@mprove/chat",
    "circular:disk": "turbo run circular --filter=@mprove/disk",
    "circular:mcli": "turbo run circular --filter=@mprove/mcli",
    "circular:front": "turbo run circular --filter=@mprove/front",
    "build:backend:prod": "turbo run build:prod --filter=@mprove/backend",
    "build:blockml:prod": "turbo run build:prod --filter=@mprove/blockml",
    "build:chat:prod": "turbo run build:prod --filter=@mprove/chat",
    "build:disk:prod": "turbo run build:prod --filter=@mprove/disk",
    "build:front:prod": "turbo run build:prod --filter=@mprove/front",
    "build:mcli:prod": "turbo run build:prod --filter=@mprove/mcli",
    "start:backend": "turbo run start --filter=@mprove/backend --log-prefix=none",
    "start:blockml": "turbo run start --filter=@mprove/blockml --log-prefix=none",
    "start:chat": "turbo run start --filter=@mprove/chat --log-prefix=none",
    "start:disk": "turbo run start --filter=@mprove/disk --log-prefix=none",
    "start:front": "turbo run start --filter=@mprove/front --log-prefix=none",
    "debug:disk": "turbo run debug --filter=@mprove/disk",
    "debug:blockml": "turbo run debug --filter=@mprove/blockml",
    "test:disk": "turbo run test --filter=@mprove/disk --log-prefix=none",
    "test:blockml": "turbo run test --filter=@mprove/blockml --log-prefix=none",
    "e2e:backend": "turbo run e2e --filter=@mprove/backend --log-prefix=none",
    "e2e:chat": "turbo run e2e --filter=@mprove/chat --log-prefix=none",
    "e2e:mcli": "pnpm clean:mcli-repos && pnpm clone:mcli && turbo run e2e --filter=@mprove/mcli --log-prefix=none",
    "clean:app": "pnpm clean:blockml-data && pnpm clean:blockml-logs && pnpm clean:clickstack-ch-data && pnpm clean:clickstack-ch-logs && pnpm clean:db-main && pnpm clean:dwh-mysql && pnpm clean:dwh-postgres && pnpm clean:mcli-repos && pnpm clean:opencode && pnpm clean:organizations",
    "clean:mprove_data": "pnpm clean:blockml-data && pnpm clean:blockml-logs && pnpm clean:clickstack-ch-data && pnpm clean:clickstack-ch-logs && pnpm clean:clickstack-db && pnpm clean:db-main && pnpm clean:dwh-mysql && pnpm clean:dwh-postgres && pnpm clean:mcli-repos && pnpm clean:opencode && pnpm clean:organizations",
    "clean:blockml-data": "rimraf --glob \"mprove_data/blockml-data/*\" \"mprove_data/blockml-data/.[!.]*\"",
    "clean:blockml-logs": "rimraf --glob \"mprove_data/blockml-logs/*\" \"mprove_data/blockml-logs/.[!.]*\"",
    "clean:clickstack-ch-data": "rimraf --glob \"mprove_data/clickstack-ch-data/*\" \"mprove_data/clickstack-ch-data/.[!.]*\"",
    "clean:clickstack-ch-logs": "rimraf --glob \"mprove_data/clickstack-ch-logs/*\" \"mprove_data/clickstack-ch-logs/.[!.]*\"",
    "clean:clickstack-db": "rimraf --glob \"mprove_data/clickstack-db/*\" \"mprove_data/clickstack-db/.[!.]*\"",
    "clean:db-main": "rimraf --glob \"mprove_data/db-main/*\" \"mprove_data/db-main/.[!.]*\"",
    "clean:dwh-mysql": "rimraf --glob \"mprove_data/dwh-mysql/*\" \"mprove_data/dwh-mysql/.[!.]*\"",
    "clean:dwh-postgres": "rimraf --glob \"mprove_data/dwh-postgres/*\" \"mprove_data/dwh-postgres/.[!.]*\"",
    "clean:mcli-repos": "rimraf --glob \"mprove_data/mcli-repos/*\" \"mprove_data/mcli-repos/.[!.]*\"",
    "clean:opencode": "rimraf --glob \"mprove_data/opencode/*\" \"mprove_data/opencode/.[!.]*\"",
    "clean:organizations": "rimraf --glob \"mprove_data/organizations/*\" \"mprove_data/organizations/.[!.]*\"",
    "clean:clickstack": "pnpm clean:clickstack-db && pnpm clean:clickstack-ch",
    "clean:clickstack-ch": "pnpm clean:clickstack-ch-data && pnpm clean:clickstack-ch-logs",
    "clean:npm": "npm cache clean --force",
    "clean": "pnpm clean:nested && pnpm clean:top",
    "clean:top": "pnpm clean:top:turbo && pnpm clean:top:pnpm-store && pnpm clean:top:node",
    "clean:top:turbo": "rimraf --glob \".turbo/*\" \".turbo/.[!.]*\"",
    "clean:top:pnpm-store": "rimraf --glob \".pnpm-store/*\" \".pnpm-store/.[!.]*\"",
    "clean:top:node": "rimraf --glob \"node_modules/*\" \"node_modules/.[!.]*\"",
    "clean:nested": "pnpm clean-turbo && pnpm clean-dist && pnpm clean-node",
    "clean-turbo": "turbo run clean-turbo",
    "clean-node": "turbo run clean-node",
    "clean-dist": "turbo run clean-dist"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@types/node": "catalog:",
    "ava": "catalog:",
    "cross-env": "catalog:",
    "drizzle-kit": "catalog:",
    "esbuild": "catalog:",
    "husky": "catalog:",
    "license-checker": "catalog:",
    "lint-staged": "catalog:",
    "madge": "catalog:",
    "prettier": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:",
    "rimraf": "catalog:",
    "shx": "catalog:",
    "ts-node": "catalog:",
    "tsconfig-paths": "catalog:",
    "tsx": "4.21.0",
    "turbo": "catalog:",
    "typescript": "catalog:"
  },
  "madge": {
    "fileExtensions": [
      "ts",
      "tsx"
    ],
    "tsConfig": "./tsconfig.json"
  },
  "lint-staged": {
    "./**/*.{css,ts,js}": [
      "biome check --write"
    ],
    "./**/*.{html,scss,json,jsonc,md}": [
      "prettier --write --config .prettierrc.js"
    ]
  },
  "vaadin": {
    "disableUsageStatistics": true
  },
  "pnpm": {
    "overrides": {
      "rxjs": "catalog:"
    }
  }
}
