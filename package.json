{
  "name": "mprove",
  "version": "8.2.1",
  "author": "Alexander Kalitenya <akalitenya@mprove.io>",
  "license": "UNLICENSED",
  "packageManager": "yarn@1.22.22",
  "private": false,
  "sideEffects": [
    "apps/front/src/polyfills.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mprove-io/mprove.git"
  },
  "bin": "./main.js",
  "scripts": {
    "postinstall": "yarn pj",
    "check:db": "node -r ts-node/register -r tsconfig-paths/register apps/backend/src/wait-for-db.ts",
    "mig:custom": "drizzle-kit generate --custom --config apps/backend/src/drizzle/postgres/drizzle.config.ts --name=$(date -u +\"%Y-%m-%d_%H-%M-%S\")",
    "mig:gen": "drizzle-kit generate --config apps/backend/src/drizzle/postgres/drizzle.config.ts --name=$(date -u +\"%Y-%m-%d_%H-%M-%S\")",
    "mig:run": "yarn node -r ts-node/register -r tsconfig-paths/register apps/backend/src/drizzle/postgres/drizzle-migrate.ts",
    "mig:dropmig": "drizzle-kit drop --config apps/backend/src/drizzle/postgres/drizzle.config.ts",
    "dz:pull": "drizzle-kit introspect --config apps/backend/src/drizzle/postgres/drizzle.config.ts",
    "dz:push": "drizzle-kit push --config apps/backend/src/drizzle/postgres/drizzle.config.ts",
    "dz:upmeta": "drizzle-kit up --config apps/backend/src/drizzle/postgres/drizzle.config.ts",
    "dz:check": "drizzle-kit check --config apps/backend/src/drizzle/postgres/drizzle.config.ts",
    "mcli": "ENV_FILE_PATH=.env yarn node -r ts-node/register -r tsconfig-paths/register apps/mcli/src/main.ts",
    "lint": "biome lint",
    "bfm": "biome check --write",
    "pfm": "prettier \"**/*.{html,scss,json,jsonc,md}\" --write --config .prettierrc.js",
    "circular": "madge --circular apps libs",
    "create-env": "ts-node ./tools/utils/create-env",
    "pj": "yarn pjc && yarn pjd",
    "pjc": "ts-node ./tools/utils/prepare-package-json-for-cli",
    "pjd": "ts-node ./tools/utils/prepare-package-json-for-docker",
    "pjc-to-dist": "shx cp package.cli.json dist/apps/mcli/package.json && shx cp yarn.lock dist/apps/mcli/yarn.lock",
    "pjc-to-tests": "shx cp package.cli.json dist/tests/mcli/package.json",
    "publish-cli": "cd dist/apps/mcli && yarn publish",
    "compose-build": "yarn pj && docker-compose build",
    "nx": "nx",
    "build:mcli": "rm -rf dist/apps/mcli/* && nx build mcli && yarn pjc-to-dist && yarn add-line:mcli",
    "build:mcli:win": "rm -rf dist/apps/mcli/* && nx build mcli && yarn pjc-to-dist",
    "add-line:mcli": "(shx echo '#!/usr/bin/env node' && shx cat dist/apps/mcli/main.js) > tempfile && shx mv tempfile dist/apps/mcli/main.js",
    "build-tests:mcli": "nx build-tests mcli && yarn pjc-to-tests",
    "clone:mcli": "git clone $MPROVE_CLI_TEST_REMOTE_GIT_URL $MPROVE_CLI_TEST_DESTINATION_URL",
    "build:prod": "nx run-many --target=build -c production --all",
    "build:backend:prod": "nx build backend -c production",
    "build:blockml:prod": "nx build blockml -c production",
    "build:disk:prod": "nx build disk -c production",
    "build:front:prod": "nx build front -c production",
    "serve:disk": "nx serve disk --port 9230",
    "serve:blockml": "nx serve blockml --port 9231",
    "serve:backend": "BACKEND_IS_SCHEDULER=TRUE nx serve backend --port 9232",
    "serve:front:dev": "nx serve front -c development",
    "serve:front:dev:host": "nx serve front -c development --host 0.0.0.0 --disable-host-check",
    "test": "nx run-many --target=test --all",
    "test:disk": "IS_TELEMETRY_ENABLED=FALSE nx test disk --verbose",
    "test:blockml": "IS_TELEMETRY_ENABLED=FALSE nx test blockml --verbose",
    "e2e": "nx run-many --target=e2e --all",
    "e2e:backend": "IS_TELEMETRY_ENABLED=FALSE nx e2e backend --verbose",
    "e2e:mcli": "IS_TELEMETRY_ENABLED=FALSE yarn clean:mcli-repos && yarn clone:mcli && nx e2e mcli --verbose",
    "make:dist": "mkdir -p dist/apps/disk dist/apps/backend dist/apps/blockml dist/apps/front",
    "clean": "yarn clean:mprove_data && yarn clean:dist && yarn clean:angular && yarn clean:nx",
    "clean:mprove_data": "rm -rf mprove_data && mkdir -p mprove_data mprove_data/blockml-data mprove_data/blockml-logs mprove_data/db-main mprove_data/dwh-mysql mprove_data/dwh-postgres mprove_data/mcli-repos mprove_data/organizations mprove_data/clickstack-db mprove_data/clickstack-ch-data mprove_data/clickstack-ch-logs",
    "clean:clickstack": "rm -rf mprove_data/clickstack-db/* && rm -rf mprove_data/clickstack-ch-data/* && rm -rf mprove_data/clickstack-ch-logs/*",
    "clean:clickstack-ch": "rm -rf mprove_data/clickstack-ch-data/* && rm -rf mprove_data/clickstack-ch-logs/*",
    "clean:app": "yarn clean:blockml-data && yarn clean:blockml-logs && yarn clean:clickstack-ch-data && yarn clean:clickstack-ch-logs && yarn clean:db-main && yarn clean:dwh-mysql && yarn clean:dwh-postgres && yarn clean:mcli-repos && yarn clean:organizations",
    "clean:blockml-data": "rm -rf mprove_data/blockml-data/*",
    "clean:blockml-logs": "rm -rf mprove_data/blockml-logs/*",
    "clean:clickstack-ch-data": "rm -rf mprove_data/clickstack-ch-data/*",
    "clean:clickstack-ch-logs": "rm -rf mprove_data/clickstack-ch-logs/*",
    "clean:db-main": "rm -rf mprove_data/db-main/*",
    "clean:dwh-mysql": "rm -rf mprove_data/dwh-mysql/*",
    "clean:dwh-postgres": "rm -rf mprove_data/dwh-postgres/*",
    "clean:mcli-repos": "rm -rf mprove_data/mcli-repos/*",
    "clean:organizations": "rm -rf mprove_data/organizations/*",
    "clean:dist": "yarn make:dist && rm -rf dist/apps/disk/* dist/apps/mcli/* dist/apps/backend/* dist/apps/blockml/* dist/apps/front/* dist/tests/* dist/bin/*",
    "clean:angular": "rm -rf .angular/*",
    "clean:nx": "rm -rf .nx/*",
    "clean:node_modules": "rm -rf ./node_modules/* && rm -rf ./node_modules/.cache/* && rm -rf ./node_modules/.bin/* && rm -rf ./node_modules/.yarn-integrity",
    "clean:npm": "npm cache clean --force",
    "pacx:mcli": "caxa --input dist/apps/mcli --output dist/bin/mp -- \"{{caxa}}/node_modules/.bin/node\" \"{{caxa}}/main.js\"",
    "pacx:mcli:win": "caxa --input dist/apps/mcli --output dist/bin/mp.exe -- \"{{caxa}}/node_modules/.bin/node\" \"{{caxa}}/main.js\""
  },
