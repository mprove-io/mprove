{
  "name": "mprove",
  "author": "Alexander Kalitenya <akalitenya@mprove.io>",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mprove-io/mprove.git"
  },
  "scripts": {
    "typeorm": "node -r ts-node/register -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "mig:drop": "npm run typeorm schema:drop",
    "mig:run": "npm run typeorm migration:run",
    "mig:gen": "npm run typeorm migration:generate -- -n All",
    "mig": "npm run mig:drop && npm run mig:run && npm run mig:gen && npm run mig:run",
    "nx": "nx",
    "x:fm": "prettier \"**/*.{ts,js,json,scss,html,md}\" --write --config .prettierrc.yaml",
    "x:circular": "madge --circular {apps,libs}",
    "x:lint": "eslint \"**/*.ts\"",
    "x": "npm run x:fm && npm run x:circular && npm run x:lint",
    "fm": "nx run-many --target=fm --all",
    "circular": "nx run-many --target=circular --all",
    "lint": "nx run-many --target=lint --all",
    "build-prod": "nx run-many --target=build --configuration=production --all",
    "pj": "nx pj top",
    "compose-build": "nx run-many --target=compose-build --all",
    "test": "nx run-many --target=test --all",
    "compose-up-build": "nx run-many --target=compose-up-build --all",
    "e2e": "nx run-many --target=e2e --all",
    "steps": "nx steps top",
    "steps-short": "nx steps-short top",
    "test-plus-e2e": "nx test-plus-e2e top",
    "steps-all": "nx steps-all top",
    "steps-all-short": "nx steps-all-short top",
    "postinstall": "ngcc --properties es2015 browser module main && node ./scripts/ngx-tailwind-jit-transform.js",
    "start:dev": "cross-env TAILWIND_MODE=watch nx serve front"
  },
